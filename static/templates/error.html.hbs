{{#*inline "body"}}
<div class="pf-c-card" id="card-basic-example">
  <div class="pf-c-card__title">{{ user }}</div>
  <div class="pf-c-card__body pf-l-stack pf-m-gutter">
    <div class="pf-l-stack__item">
      <div class="pf-c-alert pf-m-danger">
        <div class="pf-c-alert__icon">
          <i class="fas fa-fw fa-exclamation-circle" aria-hidden="true"></i>
        </div>
        <p class="pf-c-alert__title">Error!</p>
        <div class="pf-c-alert__description">
            <p>{{ message }}</p>
        </div>
      </div>
    </div>
    <div class="pf-l-stack__item">
      <a href="/manage" class="pf-c-button pf-m-link"><i class="fa fa-angle-left"></i> Go back</a>
    </div>
  </div>
</div>
{{/inline}}
{{> base.html.hbs}}
